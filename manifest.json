{
  "name": "Descarga Masiva Facturas XML CFDI SAT México",
  "description": "Descarga masivamente los CFDI (XML y PDF) del SAT México",
  "version": "0.23",
  "minimum_chrome_version": "16.0.884",
  "permissions": [
    "downloads",
    "activeTab"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://portalcfdi.facturaelectronica.sat.gob.mx/*"
  ],
  "background": {
    "service_worker": "popup.js"
  },
  "manifest_version": 3
}
